<%- include("./layouts/head.ejs",{title:"Vietpro Shopping"})%>
<%- include("./layouts/header.ejs")%>
<%- include("./layouts/menu.ejs")%>
<%- include("./layouts/slider.ejs")%>

                
                <!--	Feature Product	-->
                <div class="products">
                    <h3>Sản phẩm nổi bật</h3>
                    <div class="product-list card-deck">
                        <% for(featured of featuredProduct){%>
                        <div class="product-item card text-center">
                            <a href="/product-<%=featured.slug%>.<%=featured._id%>"><img src="../images/<%=featured.thumbnail%>"></a>
                            <h4><a href="/product-<%=featured.slug%>.<%=featured._id%>"><%=featured.name%></a></h4>
                            <p>Giá Bán: <span><%=featured.price%>đ</span></p>
                        </div>
                        <% }%>

                    </div>
                </div>
                <!--	End Feature Product	-->
                
                
                <!--	Latest Product	-->
                <div class="products">
                    <h3>Sản phẩm mới</h3>
                    <div class="product-list card-deck">
                        <% for(latest of latestProduct){%>
                        <div class="product-item card text-center">
                            <a href="/product-<%=latest.slug%>.<%=latest._id%>"><img src="../images/<%=latest.thumbnail%>"></a>
                            <h4><a href="/product-<%=latest.slug%>.<%=latest._id%>"><%=latest.name%></a></h4>
                            <p>Giá Bán: <span><%=latest.price%>đ</span></p>
                        </div>
                        <% }%>

                    </div>
                </div>
                <!--	End Latest Product	-->
                
<%- include("./layouts/sidebar.ejs")%>
<%- include("./layouts/footer.ejs")%>













</body>
</html>
